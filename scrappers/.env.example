# Example environment variables
# Copy this to .env and fill in your values

# Local LLM Settings (optional - uses Ollama locally, no API key needed)
# OLLAMA_BASE_URL=http://localhost:11434
# OLLAMA_GENERAL_MODEL=llama3.1:8b
# OLLAMA_SQL_MODEL=sqlcoder:7b
# OLLAMA_FAST_MODEL=phi3:mini

# Copart Portal
COPART_DATABASE=your_database_name

# Browser Settings
HEADLESS_MODE=false
IMPLICIT_WAIT=10

# Chrome Profile (if you want to use existing session)
CHROME_USER_DATA_DIR=
CHROME_PROFILE_DIRECTORY=
